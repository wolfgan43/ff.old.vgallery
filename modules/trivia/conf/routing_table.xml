<?xml version="1.0" encoding="utf-8"?>
<configdata>
	<rule>
		<priority>NORMAL</priority>
		<source>^/restricted/trivia(.*)</source>
		<destination>
			<module>trivia</module>
			<url>/restricted/trivia$1</url>
		</destination>
        <reverse>/restricted/trivia</reverse> 
		<accept_path_info />
	</rule>
    <rule id="trivia_services">
        <priority>NORMAL</priority>
        <source>^/services(.*)</source>
        <destination>
            <module>trivia</module>
            <url>/services$1</url>
        </destination>
        <reverse>/services</reverse>
        <accept_path_info />
    </rule>
	<rule id="trivia">
		<priority>NORMAL</priority>
		<source>^/gioca(.*)</source>
		<destination>
			<module>trivia</module>
			<url>/game$1</url>
		</destination>
        <reverse>/gioca</reverse>   
		<accept_path_info />
	</rule>
</configdata>

