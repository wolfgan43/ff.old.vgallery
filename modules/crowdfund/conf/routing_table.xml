<?xml version="1.0" encoding="utf-8"?>
<configdata>
	<rule id="crowdfund">
		<priority>NORMAL</priority>
		<source>^/restricted/crowdfund(.*)</source>
		<destination>
			<module>crowdfund</module>
			<url>/restricted/crowdfund$1</url>
		</destination>
        <reverse>/restricted/crowdfund</reverse> 
		<accept_path_info />
	</rule>
    <rule id="crowdfund_services">
        <priority>NORMAL</priority>
        <source>^/services(.*)</source>
        <destination>
            <module>crowdfund</module>
            <url>/services$1</url>
        </destination>
        <reverse>/services</reverse>
        <accept_path_info />
    </rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/restricted/crowdfund/anagraph(.*)</source>
        <destination>
            <url>/conf/gallery/ecommerce/anagraph$1</url>
        </destination>
        <accept_path_info />
    </rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/restricted/crowdfund/anagraph(.*)/modify$</source>
        <destination>
            <url>/conf/gallery/ecommerce/anagraph/modify$1</url>
        </destination>
        <accept_path_info />
    </rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/restricted/crowdfund/anagraph(.*)/preview$</source>
        <destination>
            <url>/conf/gallery/ecommerce/anagraph/preview$1</url>
        </destination>
        <accept_path_info />
    </rule>
	<rule id="crowdfund_public">
		<priority>NORMAL</priority>
		<source>^/idea(.*)</source>
		<destination>
			<module>crowdfund</module>
			<url>/idea$1</url>
		</destination>
        <reverse>/idea</reverse> 
		<accept_path_info />
	</rule>
	<rule id="crowdfund_invest">
		<priority>NORMAL</priority>
		<source>^/invest/(.*)</source>
		<destination>
			<module>crowdfund</module>
			<url>/restricted/crowdfund/idea/backers/modify/</url>
		</destination>
        <reverse>/invest</reverse> 
		<accept_path_info />
	</rule>
	<rule id="crowdfund_donate">
		<priority>NORMAL</priority>
		<source>^/donate/(.*)</source>
		<destination>
			<module>crowdfund</module>
			<url>/restricted/crowdfund/idea/backers/modify/$1</url>
		</destination>
        <reverse>/donate</reverse> 
		<accept_path_info />
	</rule>
	<rule id="crowdfund_reward">
		<priority>NORMAL</priority>
		<source>^/reward/(.*)</source>
		<destination>
			<module>crowdfund</module>
			<url>/restricted/crowdfund/idea/backers/modify/$1</url>
		</destination>
        <reverse>/reward</reverse> 
		<accept_path_info />
	</rule>

    <rule id="user_crowdfund_dashboard">
        <priority>NORMAL</priority>
        <source>^/user$</source>
        <destination>
            <module>crowdfund</module>
            <url>/restricted/crowdfund/dashboard$1</url>
        </destination>
        <reverse>/user</reverse> 
        <accept_path_info />
    </rule>
    <rule id="user_crowdfund_idea">
        <priority>NORMAL</priority>
        <source>^/user/idea(.*)</source>
        <destination>
            <module>crowdfund</module>
            <url>/restricted/crowdfund/idea$1</url>
        </destination>
        <reverse>/user/idea</reverse> 
        <accept_path_info />
    </rule>
    <rule id="user_crowdfund_info">
        <priority>NORMAL</priority>
        <source>^/info(.*)</source>
        <destination>
            <module>crowdfund</module>
            <url>/restricted/crowdfund/info$1</url>
        </destination>
        <reverse>/info</reverse> 
        <accept_path_info />
    </rule>
    <rule id="crowdfund_idea_new">
        <priority>NORMAL</priority>
        <source>^/user/idea/modify(.*)</source>
        <destination>
            <module>crowdfund</module>
            <url>/restricted/crowdfund/idea/modify$1</url>
        </destination>
        <reverse>/user/idea/modify</reverse> 
        <accept_path_info />
    </rule>
    <rule id="crowdfund_anagraph_company">
        <priority>NORMAL</priority>
        <source>^/user/anagraph(.*)/modify$</source>
        <destination>
            <url>/conf/gallery/ecommerce/anagraph/modify$1</url>
        </destination>
        <reverse>/user/anagraph</reverse>
        <accept_path_info />
    </rule>
</configdata>

