<?xml version="1.0" encoding="utf-8"?>
<configdata>
	<rule id="attendance">
		<priority>NORMAL</priority>
		<source>^/restricted/attendance(.*)</source>
		<destination>
			<module>attendance</module>
			<url>/restricted/attendance$1</url>
		</destination>
        <reverse>/restricted/attendance</reverse> 
		<accept_path_info />
	</rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/restricted/attendance/anagraph(.*)</source>
        <destination>
            <url>/conf/gallery/ecommerce/anagraph$1</url>
        </destination>
        <accept_path_info />
    </rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/restricted/attendance/anagraph(.*)/modify$</source>
        <destination>
            <url>/conf/gallery/ecommerce/anagraph/modify$1</url>
        </destination>
        <accept_path_info />
    </rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/restricted/attendance/anagraph(.*)/preview$</source>
        <destination>
            <url>/conf/gallery/ecommerce/anagraph/preview$1</url>
        </destination>
        <accept_path_info />
    </rule>
</configdata>

