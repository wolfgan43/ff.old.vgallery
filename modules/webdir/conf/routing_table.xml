<?xml version="1.0" encoding="utf-8"?>
<configdata>
	<rule id="webdir">
		<priority>NORMAL</priority>
		<source>^/restricted/webdir(.*)</source>
		<destination>
			<module>webdir</module>
			<url>/restricted/webdir$1</url>
		</destination>
        <reverse>/restricted/webdir</reverse> 
		<accept_path_info />
	</rule>
	<rule id="webdir_frontend">
		<priority>NORMAL</priority>
		<source>^(.*)</source>
		<destination>
			<module>webdir</module>
			<url>/webdir$1</url>
		</destination>
        <reverse>/</reverse> 
		<accept_path_info />
		<process_next />
	</rule>
	<rule id="user_webdir">
		<priority>NORMAL</priority>
		<source>^/user/webdir(.*)</source>
		<destination>
			<module>webdir</module>
			<url>/restricted/webdir/company/modify$1</url>
		</destination>
        <reverse>/user/webdir</reverse> 
		<accept_path_info />
	</rule>
</configdata>

