<div class="notification {{class}} {{delivered}}" >
    <div class="notification-image-wrapper">
        <div class="notification-image" data-if="cover">
            {{cover}}
        </div>
    </div>
    <div class="notification-text">
        <div class="highlight" data-if="title">{{title}}</div>
        <div class="text" >{{message}}</div>
        <div class="time">{{created}}</div>
    </div>
</div>