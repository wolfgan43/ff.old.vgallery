/* Aggiungi blocco */

.add-block .item { overflow:hidden; }

.add-block A { 
	padding:0.8em;
	height:100px;
	text-align:center;
	vertical-align:middle;
	transition:0.3s;
	background-clip: content-box;
	display:table;
	z-index:1;
}

.add-block A:hover { 
	transform:scale(1.08);
	z-index:2;
}

.add-block A>SPAN P { display:none; }

.add-block A>I, .add-block A>SPAN {
	display:table-cell;
	height:100%;
	vertical-align:middle;
	color:#fff;
}

.add-block A>I { font-size:4em; }
.add-block A.priority-3>I { font-size:4.5em !important; }
.add-block A>SPAN { font-size:1.5em; padding:0.3em; text-align:left;}
.add-block A.priority-3>SPAN P { display:block; font-size:0.5em; margin-bottom:0 !important; }

.add-block A.priority-2,
.add-block A.priority-2 >SPAN,
.add-block A.priority-2>I { display:inline-block; }
.add-block A.priority-2>SPAN P { display:none; } 
  
