.ui-icon-closethick:before, .ui-icon-closethick:after {
	content:"";
	display:block;
	position:absolute;
	top:0;
	left:6px;
	width:2px;
	height:15px;
	background:rgba(0,0,0,0.5);
	-webkit-transform:rotate(45deg);
	-ms-transform:rotate(45deg);
	transform:rotate(45deg);
	-webkit-transition:background 0.3s;
	transition:background 0.3s;
}
.ui-icon-closethick:after {-webkit-transform:rotate(-45deg); -ms-transform:rotate(-45deg); transform:rotate(-45deg);}

.ui-dialog-titlebar-close:hover .ui-icon-closethick:before, .ui-dialog-titlebar-close:hover .ui-icon-closethick:after { background:rgba(0,0,0,1); }

