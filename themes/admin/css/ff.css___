/* Tab sub-title */

.dlgTab {
	text-align:center;
}

.dlgTab A, .dlgTab A:hover, .dlgTab A:active, .dlgTab A:visited  {
	display:inline-block;
	border-bottom:6px solid transparent;
	color:#fff;
	padding:0 2em 0.5em;
	text-align:center;
	box-sizing: border-box;
}

.dlgTab A.selected {
	border-bottom:6px solid #fff;
}

/* menu-settings (menu verticale stile chrome) */

.menu-settings {
	float:left;
	margin-left:-12px !important;
}

div[class^="ff"] .menu-settings + div {
	height:100%;
	overflow:auto;
}

/* modify vgallery nodes */

.ffRecord>DIV { position:relative; }


.text-nowrap {
	white-space: nowrap;
}


.text-overflow {
	overflow: hidden;
	text-overflow :ellipsis;
}
.text-lowercase {
	text-transform: lowercase;
}
.text-uppercase {
	text-transform: uppercase;
}
.text-capitalize {
	text-transform: capitalize;
}
.disabled {
	opacity: 0.4;
	pointer-events: none;
}
