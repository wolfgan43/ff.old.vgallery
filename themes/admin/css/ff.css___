/* label */
.ffRecord .line LABEL,
.ffGrid .line LABEL,
.ffDetails .line LABEL {
	display:block;
	margin-bottom:4px;
}

INPUT.detailQTA + LABEL{
	display:inline;
}

/* input string */
.ffRecord INPUT,
.ffGrid INPUT,
.ffDetails INPUT,
.ffRecord SELECT,
.ffGrid SELECT,
.ffDetails SELECT,
.ffRecord TEXTAREA,
.ffRecord LABEL+SPAN.readonly,
.ffGrid LABEL+SPAN.readonly,
.ffDetails LABEL+SPAN.readonly {
	font-family:Verdana;
	line-height:3em;

	font-size:1em;
	padding:0 5px;
	border:1px solid #c6c6c6;
	width:100%;
	box-sizing:border-box;
	display:inline-block;
}

.ffRecord LEGEND SELECT { width:auto !important; margin:0 !important; line-height:1em !important; } 

TD.number  { min-width:4.2em; }
.number INPUT { 
	height:2.3em !important;
	text-align:right;
	margin-right:0.3em;
	border-width:0 0 1px 0 !important;
	padding:0 !important;
}

.ffRecord LABEL+SPAN.readonly,
.ffGrid LABEL+SPAN.readonly,
.ffDetails LABEL+SPAN.readonly {
	color:#666;
	background-color:#fafafa;
	cursor:default;
}

.ffRecord INPUT[type="checkbox"],
.ffGrid  INPUT[type="checkbox"],
.ffDetails  INPUT[type="checkbox"],
.ffRecord INPUT.radio,
.ffGrid  INPUT.radio,
.ffDetails  INPUT.radio {width:auto; height:auto; }


INPUT.detailQTA { width:3em; vertical-align:bottom;}

div:not(.autocomplete) TEXTAREA,
	div:not(.block) TEXTAREA {
	min-width:300px;
}

SELECT.ffWidget_timepicker {
	width:49% !important;
	display:block;
	float:left;
}

SELECT.ffWidget_timepicker + SPAN {
	display:block;
	float:left;
	width:2%;
	text-align:center;
	line-height:3em;
}

/* slider */

