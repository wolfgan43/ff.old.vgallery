    cursor: nw-resize;
    width: 9px;
    height: 9px;
    left: -5px;
    top: -5px;
}

.ui-resizable-ne {
    cursor: ne-resize;
    width: 9px;
    height: 9px;
    right: -5px;
    top: -5px;
}






.ffRecord .line,
.ffGrid .line {
	margin:1em 0;
	clear:both;
        height:auto;
}

.timepicker { overflow:hidden; } /* line che contiene elementi in float, ha bisogno dell'overflow per avere un altezza'  */

.line.autocompletetoken {
	height:4em !important; /* 1em di label + 3em di input */
	padding-bottom:4px; /* il padding della label */
