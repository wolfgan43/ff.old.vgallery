/* sorting stuff */
.ffGrid .heading .sort-block {
	margin: 0 5px;
	float: left;
	position: relative;
}
.ffGrid .heading .sort-block LABEL {
	line-height: 32px;
	color: #888;
	margin: 0 4px 0 0;
}
.ffGrid .heading .sort-block a.dropdown-sort {
	border: 1px solid #d9d9d9;
	-moz-border-radius: 2px;
	-webkit-border-radius: 2px;
	border-radius:2px;
	padding: 8px 24px 8px 16px;
	line-height:32px;
	background: url(../images/admin-ico.png) 142px -56px no-repeat;
}
.ffGrid .heading .dropdown-sort-data UL {
	position: absolute;
	z-index: 100;
	top: 32px;
	left: 64px;
	width:160px !important;
	padding:0;
	background:#fff;
	border:1px solid #cbcbcb;
	-moz-box-shadow: 1px 1px 3px #ddd;
	-webkit-box-shadow: 1px 1px 3px #ddd;
	box-shadow: 1px 1px 3px #ddd;
}
.ffGrid .heading .dropdown-sort-data UL LI {
	clear: both;
}
.ffGrid .heading .dropdown-sort-data UL LI A {
	padding:5px !important;
	display: block;
	width: 150px !important;
	font-size: 12px !important;	
	border-bottom: 1px solid #f8f8f8;	
}
.ffGrid .heading .dropdown-sort-data UL LI A:hover {
	background: #fff0be;
	border-bottom: 1px solid #f1dd9f;
}
.ffGrid .heading .dropdown-sort-div {
	float: right;
}
/* fine sort */


/* tabelle */

.ffGrid span.data{
    font-size: 12px;
    line-height:14px;
}

.ffGrid A.add{
    float:none;
    padding:0;
    margin:0;
}

#VGalleryNodes_discl_sect { clear:both; }

table.ffGrid {
	width:100%;
}
table.ffGrid TR TH {
	padding: 6px;
	font-weight: normal;
	color: #52627e;
	background-image: linear-gradient(bottom, rgb(247,247,247) 9%, rgb(255,255,255) 71%);
	background-image: -o-linear-gradient(bottom, rgb(247,247,247) 9%, rgb(255,255,255) 71%);
	background-image: -moz-linear-gradient(bottom, rgb(247,247,247) 9%, rgb(255,255,255) 71%);
	background-image: -webkit-linear-gradient(bottom, rgb(247,247,247) 9%, rgb(255,255,255) 71%);
	background-image: -ms-linear-gradient(bottom, rgb(247,247,247) 9%, rgb(255,255,255) 71%);
	background-image: -webkit-gradient( linear, left bottom, left top, color-stop(0.09, rgb(247,247,247)), color-stop(0.71, rgb(255,255,255)) );
	border-bottom: 1px solid #e5e5e5;
	border-top: 1px solid #f7f7f7;
	text-align: left;
}
table.ffGrid TR TH.ffButton {
	padding: 6px 0;
}
table.ffGrid TR TH A {
	color: #52627e;
}
table.ffGrid TR.negative {
	background:#f5f5f5;
}
table.ffGrid TBODY TR:hover {
	background: #fce8ac;
}

table.ffGrid TD {
	vertical-align: middle;
	padding:6px 2px;
	color: #333;
}
table.ffGrid TD.ffButton {
	padding: 6px 0;
}
table.ffGrid TD A {
	color: #333;
}
table.ffGrid .ffField {
	border-right:1px solid #e9ecf2;
}
table.ffGrid TBODY TR:hover .ffField {
	border-right:1px solid #e9cf7f;
}
.ffGrid IMG.avatar {
	width:50px;
	height:50px;
	background: url(../images/empty-avatar.png) no-repeat;
} 

.ffGrid SPAN.ascending, .ffGrid SPAN.descending {
	display:inline-block;
	width:18px;
	height:15px;
	background:url(../images/FormsGrid/ascending.png) 0 4px no-repeat;
}

.ffGrid SPAN.descending { background:url(../images/FormsGrid/descending.png) 0 4px no-repeat; }

/* page navigator */

.ffGrid .FormsGridCommands {
	clear: both;
}

.ffGrid .FormsGridCommands.cleared.top {
	width:100%;
	overflow:hidden;
	
}
.pageNavigator .perPage, .pageNavigator .pages, .pageNavigator .choice, .pageNavigator .totelem {
	float: right;
	width:auto;
	padding: 4px 6px;
	line-height: 18px;
	color: #6e6e6e;
	margin: 4px;
	border: 1px solid #d9d9d9;
	border-radius:3px;
	-webkit-border-radius:3px;
	-moz-border-radius:3px;
	background-image: linear-gradient(bottom, rgb(241,241,241) 1%, rgb(245,245,245) 51%);
	background-image: -o-linear-gradient(bottom, rgb(241,241,241) 1%, rgb(245,245,245) 51%);
	background-image: -moz-linear-gradient(bottom, rgb(241,241,241) 1%, rgb(245,245,245) 51%);
	background-image: -webkit-linear-gradient(bottom, rgb(241,241,241) 1%, rgb(245,245,245) 51%);
	background-image: -ms-linear-gradient(bottom, rgb(241,241,241) 1%, rgb(245,245,245) 51%);
	background-image: -webkit-gradient( linear, left bottom, left top, color-stop(0.01, rgb(241,241,241)), color-stop(0.51, rgb(245,245,245)) );
}
.pageNavigator .perPage:hover, .pageNavigator .pages:hover, .pageNavigator .choice:hover, .pageNavigator .totelem:hover {
	border: 1px solid #c6c6c6;
	-moz-box-shadow: 0 0 3px #ddd;
	-webkit-box-shadow: 0 0 3px #ddd;
	box-shadow: 0 0 3px #ddd;
}
.pageNavigator .pages A { color: #666; letter-spacing: 1px; float: left; height: 18px; margin:0 5px 0 0; }
.pageNavigator .pages A.current { color: #db4a37; }
.pageNavigator .pages A.first:hover { background:url(../images/admin-ico.png) -96px -28px no-repeat; }
.pageNavigator .pages A.prev:hover { background:url(../images/admin-ico.png) -72px -28px no-repeat; }
.pageNavigator .pages A.prev-frame:hover { background:url(../images/admin-ico.png) -122px -28px no-repeat; }
.pageNavigator .pages A.next:hover { background:url(../images/admin-ico.png) -196px -28px no-repeat; }
.pageNavigator .pages A.next-frame:hover { background:url(../images/admin-ico.png) -145px -28px no-repeat; }
.pageNavigator .pages A.last:hover { background:url(../images/admin-ico.png) -172px -28px no-repeat; }

.pageNavigator .pages A.first { background:url(../images/admin-ico.png) -96px -60px no-repeat; }
.pageNavigator .pages A.prev { background:url(../images/admin-ico.png) -72px -60px no-repeat; }
.pageNavigator .pages A.prev-frame { background:url(../images/admin-ico.png) -122px -60px no-repeat; }
.pageNavigator .pages A.next { background:url(../images/admin-ico.png) -196px -60px no-repeat; }
.pageNavigator .pages A.next-frame { background:url(../images/admin-ico.png) -145px -60px no-repeat; }
.pageNavigator .pages A.last { background:url(../images/admin-ico.png) -172px -60px no-repeat; }

.pageNavigator .pages A.first.disabled { background:url(../images/admin-ico.png) -96px -43px no-repeat; }
.pageNavigator .pages A.prev.disabled { background:url(../images/admin-ico.png) -72px -43px no-repeat; }
.pageNavigator .pages A.prev-frame.disabled { background:url(../images/admin-ico.png) -122px -43px no-repeat; }
.pageNavigator .pages A.next.disabled { background:url(../images/admin-ico.png) -196px -43px no-repeat; }
.pageNavigator .pages A.next-frame.disabled { background:url(../images/admin-ico.png) -145px -43px no-repeat; }
.pageNavigator .pages A.last.disabled { background:url(../images/admin-ico.png) -172px -43px no-repeat; }
.pageNavigator .choice INPUT.currentpage {
	height: 14px;
	border: 1px solid #ddd;
}
.pageNavigator .perPage a {
	background: #e5e5e5;
	color: #666;
	height: 18px;
	line-height: 18px;
	padding: 2px 4px;
	border: 1px solid #dbdbdb;
	-moz-border-radius: 2px;
	-webkit-border-radius: 2px;
	border-radius:2px;
}
.pageNavigator .perPage a:hover {
	background: #f4f4f4;
}
.pageNavigator .perPage .current {
	background: #3f84fb;
	border:1px solid #1d73f7;
	color: #fff;
}


