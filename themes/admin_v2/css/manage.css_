/* input */
.input,
.combo,
.password,
.ffWidget_timepicker,
.ffWidget_activecomboex,
.ffWidget_datepicker_text,
.detailQTA,
.ricerca-semplice .input {
	-webkit-appearance: none;
	border: 1px solid #d9d9d9;
	-moz-border-radius: 2px;
	-webkit-border-radius: 2px;
	border-radius:2px;
	padding: 6px 6px 3px;
}

.combo {-webkit-appearance: menu;}

.input:focus,
.textarea:focus,
.combo:focus,
.password:focus,
.ffWidget_activecomboex:focus,
.ffWidget_datepicker_text:focus,
.ffWidget_timepicker:focus,
.detailQTA:focus{
	border: 1px solid #2775f1;	
}

/* search */
.heading .search { position: relative; margin-bottom:12px; clear:left; width:470px; float:left;}

.ricerca-semplice {
	width: 460px;
	position: relative;
}

.ricerca-semplice .input {
	padding: 8px 4px;
	width: 368px;
}

.ricerca-semplice .noactivebuttons {
	float: right;
	background: #3f84fb;
	padding: 6px 14px 8px;
	border: 1px solid #1d72f7;
}

.ricerca-semplice .noactivebuttons:hover {
	background: #2876f4;
	border: 1px solid #2655bf;
}

.ricerca-semplice .simple {
	width: 16px;
	position: absolute;
	top: 12px;
	left: 360px;
}

.search_more {
	display: block;
	width: 16px;
	height: 16px;
	line-height: 0;
	font-size: 0;
	text-indent: -10000px;
	background: url(../images/admin-ico.png) -4px -50px no-repeat;
}

.search_more:hover {
	background: url(../images/admin-ico.png) -4px -66px no-repeat;
}

.search .set {
	width: 432px;
	padding: 10px;
	background: #fff;
	position: absolute;
	z-index: 100;
	top: 32px;
	left: 0;
	border: 1px solid #cccccc;
	-moz-box-shadow: 2px 3px 5px #AAA;
	-webkit-box-shadow: 2px 3px 5px #AAA;
	box-shadow: 2px 3px 5px #AAA;
}

.search .set LABEL {
	display: block;
	font-size: 14px;
	color: #666;
	line-height: 28px;
}

.search .set .ffWidget_activecomboex,
.search .set .hasDatepicker,
