/* bottoni */

.ffGrid .heading .actions {
	float:right;
	margin:0 10px;
	clear:none;
}

A.cancel,
A.addNew,
.detailSubmit,
.activebuttons,
.noactivebuttons,
.heading .actions A {
	background: #d84b31;
	background-image: linear-gradient(bottom, rgb(216,74,49) 18%, rgb(216,78,43) 64%, rgb(224,80,44) 80%);
	background-image: -o-linear-gradient(bottom, rgb(216,74,49) 18%, rgb(216,78,43) 64%, rgb(224,80,44) 80%);
	background-image: -moz-linear-gradient(bottom, rgb(216,74,49) 18%, rgb(216,78,43) 64%, rgb(224,80,44) 80%);
	background-image: -webkit-linear-gradient(bottom, rgb(216,74,49) 18%, rgb(216,78,43) 64%, rgb(224,80,44) 80%);
	background-image: -ms-linear-gradient(bottom, rgb(216,74,49) 18%, rgb(216,78,43) 64%, rgb(224,80,44) 80%);
	background-image: -webkit-gradient( linear, left bottom, left top, color-stop(0.18, rgb(216,74,49)), color-stop(0.64, rgb(216,78,43)), color-stop(0.8, rgb(224,80,44)));	
	color: #fff;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	border-radius:4px;
	padding: 8px 6px;
	cursor: pointer;
}

A.cancel:hover,
A.addNew:hover,
.detailSubmit:hover,
.activebuttons:hover,
.noactivebuttons:hover,
.heading .actions A:hover {
	background-image: linear-gradient(bottom, rgb(198,57,32) 44%, rgb(223,77,51) 72%);
	background-image: -o-linear-gradient(bottom, rgb(198,57,32) 44%, rgb(223,77,51) 72%);
	background-image: -moz-linear-gradient(bottom, rgb(198,57,32) 44%, rgb(223,77,51) 72%);
	background-image: -webkit-linear-gradient(bottom, rgb(198,57,32) 44%, rgb(223,77,51) 72%);
	background-image: -ms-linear-gradient(bottom, rgb(198,57,32) 44%, rgb(223,77,51) 72%);
	background-image: -webkit-gradient( linear, left bottom, left top, color-stop(0.44, rgb(198,57,32)), color-stop(0.72, rgb(223,77,51)) );
}

.detailSubmit,
.activebuttons,
.noactivebuttons {
	border: 0;
	padding: 6px 6px 7px;
}

.noactivebuttons.export {
	float:right;
}

A.addNew, A.addNew:link, A.addNew:visited, A.addNew:active, .heading .actions A  {
	color: #fff;
