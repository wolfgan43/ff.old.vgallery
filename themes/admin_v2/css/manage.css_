	height:16px;
	background:url(../images/admin-ico.png) -242px -61px no-repeat;
	margin:0 auto;
}
.warehouse { width:24px; }
.stockincome { background:url(../images/admin-ico.png) -264px -61px no-repeat; }
.stockavailable { background:url(../images/admin-ico.png) -280px -61px no-repeat; }
.stockrequested { background:url(../images/admin-ico.png) -296px -61px no-repeat; }
/* tabella magazzino e stock prodotto */
TABLE.location_stock {
	border:0;	
}
TABLE.location_stock TR {
	border-bottom:0;
}
/* tabella stock prodotto */
TABLE.actual_stock {
	width:50px;
	background-image:none;
	background:none;
	border:0;
}
TABLE.actual_stock THEAD {
	display:none;
}
TABLE.actual_stock TR{
	background:none;
	border-bottom:0;
}
TABLE.actual_stock TR TD {
	padding:1px 0;
	height:16px;
	vertical-align:top;
	text-align:right;
	width:16px;
	border-right:0;
	border-left:0;
}
/* legenda */
.legend {
	float:left;
	padding:12px 8px;
	width:500px;
	font-size:10px;
	color:#666;
	text-shadow:1px 1px 1px #fefefe;
}
.legend SPAN{
	vertical-align:middle;
}
/* tabella totali */
.total {
	background:#e5e5e5;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	border-radius: 2px;
	border:1px solid #cbcbcb;
	overflow:hidden;
}
.total .heading { display:none; }
.total .grid {
	padding:8px;
}
.total .grid TABLE.compactable {
	border:0;
	background:none;
}

.total .grid TABLE.compactable TR,
.total .grid TABLE.compactable TR.negative {
	background:none;
	background-image:none;
	border-bottom:0;
}
.total TABLE TBODY TR:hover {
	background: #e5e5e5;
}
.total TABLE TBODY TR:hover .ffField {
	border-right:0;
}
.total .grid TABLE  TR TH {
	border-bottom:0;
	border-right:0;
	
	font-size:14px;
	font-weight:normal;
	padding:3px;
	color:#222;
	background:#f1f1f1;
}
.total .grid TABLE TR TD {
	color:#666;
	border-left:0;
	border-right:0;
}
.toggle {
	width:110px;
	height:27px;
	margin:0 0 5px;
	float:left;
	padding:0 20px;
	font-size:14px;
	line-height:24px;
	color:#fff;
	background:url(../images/manage/bg-toggle.jpg) no-repeat;
}

/* classi generiche */
.right { text-align:right; }
.left { text-align:left; }
.center { text-align:center; }
.red { background:#fbdbdb; }
.green { background:#dbfbdd; }
.error { 
	color:#f00;
	font-weight:bold;
}

