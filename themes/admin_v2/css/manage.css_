/* forms input */
/* Buttons */

.ffRecord .actions,
.ffGrid .actions {
    margin:5px 0;
    clear:both;
    height:60px;
}
.ffRecord .actions .cancel,
.ffGrid .actions .cancel {
    margin:0;
}
.ffRecord .actions INPUT,
.ffGrid .actions INPUT {
    margin:0 5px;
}

/* Input */
.ffRecord input::-moz-focus-inner,
.ffGrid input::-moz-focus-inner,
.ffDetails input::-moz-focus-inner,
.ffRecord button::-moz-focus-inner,
.ffGrid button::-moz-focus-inner,
.ffDetails button::-moz-focus-inner {
	border:0;
}

.ffRecord .textarea,
.ffGrid .textarea,
.ffDetails .textarea {
    width:98%;
    height:150px;
	resize:none;
}


.ffRecord .ffWidget_editarea,
.ffGrid .ffWidget_editarea,
.ffDetails .ffWidget_editarea {
    width:100%;
    height:450px;
}

.ffGrid TD .text,
.ffDetails TD .text {
    width:98%;
}


.ffRecord span.activecomboex,
.ffGrid span.activecomboex,
.ffDetails span.activecomboex {
    display:inline-block;
    min-width:350px;
}

.ffRecord span.activecomboex a.add,
.ffGrid span.activecomboex a.add,
.ffDetails span.activecomboex a.add {
    display:block;
    float:right;
    padding:3px;
}

.ffRecord span.activecomboex a.qta-meno,
.ffGrid span.activecomboex a.qta-meno,
.ffDetails span.activecomboex a.qta-meno,
.ffRecord span.activecomboex a.qta-piu,
.ffGrid span.activecomboex a.qta-piu,
.ffDetails span.activecomboex a.qta-piu {
    display:block;
    width: 24px;
    float:right;
    text-align: center;
    background: #327eff;
    color: #fff;
    font-size: 20px;
    line-height: 24px;
    border-radius:4px;
    -moz-border-radius:4px;
    -webkit-border-radius: 4px;
    margin: 0 0 0 4px;
}

.ffRecord span.activecomboex SELECT,
.ffGrid span.activecomboex SELECT,
.ffDetails span.activecomboex SELECT,
.ffRecord TD .combo,
.ffGrid TD .combo, 
.ffDetails TD .combo {
    width:90%;
}

.ffRecord span.jquery-checkbox,
.ffGrid span.jquery-checkbox,
.ffDetails span.jquery-checkbox {
    display:block;
    float:right;
}

.ffRecord .ffWidget_checkgroup,
.ffGrid .ffWidget_checkgroup,
.ffDetails .ffWidget_checkgroup {
    border-bottom:1px solid #eee;
    margin:3px 0;
    overflow:hidden;
}

/* Fieldset */

.ffRecord FIELDSET.default,
.ffDetails FIELDSET.default,
.ffRecord FIELDSET,
.ffDetails FIELDSET {
    padding:3px;
    color:#666;
    clear:both;
}

.ffRecord FIELDSET.default,
.ffDetails FIELDSET.default  {
	clear:both;
}

/*
DIV#tabs_rel {
	padding:0 0 100px 0;
}

lo sostituisco con */
.ffPage-tabs {
	padding:0 0 100px 0;
}

.ffRecord DIV.row,
.ffGrid DIV.row,
.ffDetails DIV.row {
    clear:both;
    /*overflow:auto;*/
    margin:2px 0;
    padding:2px;
    border-bottom:1px dotted #eee;
}
.ffRecord DIV.row:hover,
.ffGrid DIV.row:hover,
.ffDetails DIV.row:hover {
	background: #feefc1;
    border-bottom:1px solid #e2bb46;    
}

.ffRecord DIV.row LABEL,
.ffGrid DIV.row LABEL,
.ffDetails DIV.row LABEL {
    float:left;
    display:block;
    min-width:200px;
    text-align:right;
    margin:0 5px;   
}

.ffRecord DIV.row .label,
.ffRecord DIV.row LABEL,
.ffGrid DIV.row .label,
.ffGrid DIV.row LABEL,
.ffDetails DIV.row .label,
.ffDetails DIV.row LABEL {
	display: inline-block;
    padding:8px 3px 5px;
}


.ffRecord .description,
.ffDetail .description {
	font-size:0.5em;
}

.ffRecord DIV.row LABEL .required,
.ffGrid DIV.row LABEL .required,
.ffDetails DIV.row LABEL .required {
    color:#5bcdf7;
}

.ffRecord FIELDSET.default DIV.row DIV.preview SPAN.fileName,
.ffDetails FIELDSET.default DIV.row DIV.preview SPAN.fileName {
    margin:0 0 0 167px;
}

.ffRecord .cke_wrapper,
.ffGrid .cke_wrapper,
.ffDetails .cke_wrapper {
    display:block;
    clear:both;    
}
.ffRecord .ffWidget_ckeditor,
.ffGrid .ffWidget_ckeditor,
.ffDetails .ffWidget_ckeditor {
    width: 872px;
    height: 537px;
}

