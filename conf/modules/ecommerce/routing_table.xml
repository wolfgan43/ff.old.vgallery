            <url>/conf/gallery/ecommerce/manage/modify_order_services$1</url>
        </destination>
        <accept_path_info/>
    </rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/manage/operations(.*)/modify_shipping$</source>
        <destination>
            <url>/conf/gallery/ecommerce/manage/modify_shipping$1</url>
        </destination>
        <accept_path_info/>
    </rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/manage/operations(.*)/history</source>
        <destination>
            <url>/conf/gallery/ecommerce/manage/history$1</url>
        </destination>
        <accept_path_info/>
    </rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/manage/documents(.*)</source>
        <destination>
            <url>/conf/gallery/ecommerce/documents$1</url>
        </destination>
        <accept_path_info/>
    </rule>
    <rule>
