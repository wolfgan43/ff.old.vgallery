        <source>^/manage/reports(.*)</source>
        <destination>
            <url>/conf/gallery/ecommerce/reports$1</url>
        </destination>
        <accept_path_info/>
    </rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/manage/documents/bill(.*)/modify$</source>
        <destination>
            <url>/conf/gallery/ecommerce/documents/bill/modify$1</url>
        </destination>
        <accept_path_info/>
    </rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/manage/documents/bill(.*)/preview$</source>
        <destination>
            <url>/conf/gallery/ecommerce/documents/bill/preview$1</url>
        </destination>
        <accept_path_info/>
    </rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/manage/documents/payments(.*)/modify$</source>
        <destination>
            <url>/conf/gallery/ecommerce/documents/payments/modify$1</url>
        </destination>
        <accept_path_info/>
    </rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/manage/discount/coupon(.*)</source>
        <destination>
            <url>/conf/gallery/ecommerce/coupon$1</url>
        </destination>
        <accept_path_info/>
    </rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/manage/shipping(.*)</source>
        <destination>
            <url>/conf/gallery/ecommerce/shipping$1</url>
        </destination>
        <accept_path_info/>
    </rule>
    <rule>
        <priority>NORMAL</priority>
        <source>^/manage/info(.*)</source>
        <destination>
            <url>/conf/gallery/ecommerce$1</url>
        </destination>
    </rule>
    <!--Manage End-->
</configdata>