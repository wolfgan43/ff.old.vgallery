<configdata>
	<!--Restricted Begin-->
	<rule id="restricted">
		<priority>HIGH</priority>
		<source>^/restricted(.*)</source>
		<destination>
			<module>auth</module>
			<url>/session$0</url>
		</destination>
        <auth>
            <profiles>admin,data-entry</profiles>
        </auth>
		<process_next />
		<accept_path_info />
		<reverse>/restricted</reverse>
		<index>999</index>
	</rule>
